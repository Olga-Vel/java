<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<filter>
		<filter-name>MyFilter</filter-name>
		<display-name>MyFilter</display-name>
		<description></description>
		<filter-class>br.com.statvel.linkedin.servlet.MyFilter</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>MyFilter</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>MyServlet</servlet-name>
		<display-name>MyServlet</display-name>
		<description></description>
		<servlet-class>br.com.statvel.linkedin.servlet.MyServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ReturnsJsonServlet</servlet-name>
		<display-name>ReturnsJsonServlet</display-name>
		<description></description>
		<servlet-class>br.com.statvel.linkedin.servlet.ReturnsJsonServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ContactsServlet</servlet-name>
		<display-name>ContactsServlet</display-name>
		<description></description>
		<servlet-class>br.com.statvel.linkedin.servlet.ContactsServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ProfileServlet</servlet-name>
		<display-name>ProfileServlet</display-name>
		<description></description>
		<servlet-class>br.com.statvel.linkedin.servlet.ProfileServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ErrorServlet</servlet-name>
		<display-name>ErrorServlet</display-name>
		<description></description>
		<servlet-class>br.com.statvel.linkedin.servlet.ErrorServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyServlet</servlet-name>
		<url-pattern>/</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ReturnsJsonServlet</servlet-name>
		<url-pattern>/returns-json-servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ContactsServlet</servlet-name>
		<url-pattern>/contacts</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ProfileServlet</servlet-name>
		<url-pattern>/profile</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>ErrorServlet</servlet-name>
		<url-pattern>/error</url-pattern>
	</servlet-mapping>
	
</web-app>
